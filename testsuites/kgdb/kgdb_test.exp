              send_user "!!!!!!!! Breakpoint 4 has been skipped. Error!"
              clean_host_gdb
              case_fail $case_num
              exit
              }
           }
        }

        timeout {
                clean_host_gdb
                case_fail $case_num
                        exit
        }
        }
 
        set spawn_id $kermit_spawn_id
        expect {
              -re  "L1\\\(before change\\\).*data variable addr = 0xff80\[0-9a-f]{4}.*code function addr = 0xffa0\[0-9a-f]{4}" { 
                    expect {
                    "> " {
                    clean_host_gdb
                    case_fail $case_num
                            exit
                    }
                   timeout {
                    case_pass $case_num
                    }
                    }
                }
