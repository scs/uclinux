msgid "Invalid PORT.\n"
msgstr "Virheellinen PORTti.\n"

#: src/ftp.c:766
msgid ""
"\n"
"REST failed, starting from scratch.\n"
msgstr ""
"\n"
"REST epäonnistui, aloitetaan alusta.\n"

#: src/ftp.c:827
#, c-format
msgid ""
"No such file `%s'.\n"
"\n"
msgstr ""
"Tiedostoa ”%s” ei ole.\n"
"\n"

