msgid "       --spider                  don't download anything.\n"
msgstr "       --spider                    älä nouda mitään\n"

#: src/main.c:435
msgid "  -T,  --timeout=SECONDS         set all timeout values to SECONDS.\n"
msgstr "  -T,  --timeout=SEKUNTIA          kaikkien aikakatkaisujen pituus\n"

