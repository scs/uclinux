#, c-format
msgid ""
"No such directory `%s'.\n"
"\n"
msgstr ""
"Hakemistoa ”%s” ei ole.\n"
"\n"

#. do not CWD
#: src/ftp.c:585
msgid "==> CWD not required.\n"
msgstr "==> CWD:tä ei vaadita.\n"

