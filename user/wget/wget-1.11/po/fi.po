msgstr "       --ftp-user=KÄYTTÄJÄ         FTP-käyttäjänimi\n"

#: src/main.c:570
msgid "       --ftp-password=PASS     set ftp password to PASS.\n"
msgstr "       --ftp-password=SALASANA     FTP-salasana\n"

