	  QUICK_POWER		0x02
	  NOISE_OFF		0x03
	  WIFI			0x04
	  AP_LAUNCH		0x05
	  MAXIMIZE		0x06
	  PLUS			0x07
	  REV			0x08
	  PLAY/PAUSE		0x09
	  FWD			0x0A
	  MINUS			0x0B
        end codes

