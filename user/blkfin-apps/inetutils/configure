#else
# if HAVE_STDLIB_H
#  include <stdlib.h>
# endif
#endif
#if HAVE_STRING_H
# if !STDC_HEADERS && HAVE_MEMORY_H
#  include <memory.h>
# endif
# include <string.h>
#endif
#if HAVE_STRINGS_H
# include <strings.h>
#endif
#if HAVE_INTTYPES_H
# include <inttypes.h>
#else
# if HAVE_STDINT_H
#  include <stdint.h>
# endif
#endif
#if HAVE_UNISTD_H
# include <unistd.h>
#endif"

ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os inetdaemondir ftpd_BUILD inetd_BUILD rexecd_BUILD rlogind_BUILD rshd_BUILD syslogd_BUILD talkd_BUILD telnetd_BUILD tftpd_BUILD uucpd_BUILD ftp_BUILD ping_BUILD rcp_BUILD rlogin_BUILD rsh_BUILD logger_BUILD talk_BUILD telnet_BUILD tftp_BUILD whois_BUILD ifconfig_BUILD INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA CYGPATH_W PACKAGE VERSION ACLOCAL AUTOCONF AUTOMAKE AUTOHEADER MAKEINFO AMTAR install_sh STRIP ac_ct_STRIP INSTALL_STRIP_PROGRAM AWK SET_MAKE CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT DEPDIR am__include am__quote AMDEP_TRUE AMDEP_FALSE AMDEPBACKSLASH CCDEPMODE am__fastdepCC_TRUE am__fastdepCC_FALSE CPP EGREP AR ac_ct_AR RM RANLIB ac_ct_RANLIB YACC LIBREADLINE LIBTERMCAP LIBCRYPT NCURSES_INCLUDE LIBNCURSES LIBOBJS LIBUTIL LIBPAM LIBLS libls_BUILD LIBCURSES LIBGLOB libglob_BUILD KRB5CFGPATH LIBAUTH INCAUTH LIBWRAP U ANSI2KNR ALLOCA LTLIBOBJS'
ac_subst_files='PATHDEFS_MAKE PATHS_DEFS'

# Initialize some variables set by options.
ac_init_help=
ac_init_version=false
# The variables have the same names as the options, with
# dashes changed to underlines.
cache_file=/dev/null
exec_prefix=NONE
no_create=
no_recursion=
