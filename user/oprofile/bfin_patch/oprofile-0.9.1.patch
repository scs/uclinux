-
-static void opd_24_start(void)
-{
-	op_open_files();
-
-	/* yes, this is racey. */
-	opd_get_ascii_procs();
-
-	/* simple sleep-then-process loop */
-	opd_do_read(sbuf, s_buf_bytesize, nbuf, n_buf_bytesize);
