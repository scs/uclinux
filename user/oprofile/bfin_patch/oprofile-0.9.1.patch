-/**
- * @file opd_parse_proc.c
- * Parsing of /proc/#pid
- *
- * @remark Copyright 2002 OProfile authors
- * @remark Read the file COPYING
- *
- * @author John Levon
- * @author Philippe Elie
- */
-
-#include "op_libiberty.h"
-
-#include "opd_parse_proc.h"
-#include "opd_proc.h"
-#include "opd_mapping.h"
-#include "opd_image.h"
-#include "opd_printf.h"
-
-#include "op_file.h"
