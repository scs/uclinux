-	if (separate_thread) {
-		if (image->tid != tid || image->tgid != tgid)
-			return 1;
-	}
-
-	/* if !separate_lib, the comparison made by caller is enough */
-	if (!separate_lib)
-		return 0;
-
-	if (image->app_name == NULL && app_name == NULL)
-		return 0;
-
-	if (image->app_name != NULL && app_name != NULL &&
-	    !strcmp(image->app_name, app_name))
